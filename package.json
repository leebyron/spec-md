{
  "name": "spec-md",
  "version": "1.1.1",
  "author": "Lee Byron <lee@leebyron.com> (http://leebyron.com/)",
  "license": "MIT",
  "homepage": "https://spec-md.com",
  "bugs": {
    "url": "https://github.com/leebyron/spec-md/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:leebyron/spec-md.git"
  },
  "bin": "./bin/spec-md",
  "main": "./src/index.js",
  "scripts": {
    "build": "pegjs src/grammar.pegjs && mkdir -p out && echo 'spec-md.com' > out/CNAME && ./bin/spec-md -m spec/metadata.json README.md > out/index.html",
    "test": "node ./test/runner.js",
    "watch": "nodemon -e css,js,json,pegjs,md --ignore src/grammar.js --exec 'npm run build && npm test'"
  },
  "dependencies": {
    "prismjs": ">=1.23.0",
    "terser": "^5.5.0"
  },
  "devDependencies": {
    "jest-diff": "26.6.2",
    "nodemon": "2.0.7",
    "pegjs": "~0.8.0"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "files": [
    "bin",
    "src",
    "static",
    "LICENSE"
  ],
  "keywords": [
    "ast",
    "ecma",
    "ecmascript",
    "html",
    "markdown",
    "markup",
    "spec",
    "specs",
    "specification",
    "typography",
    "code"
  ]
}
